// List all collections
GET collections

// Get collection info
GET collections/justicio

// Create a new collection
// https://qdrant.tech/documentation/tutorials/bulk-upload/
PUT /collections/{collection_name}
{
    "vectors": {
      "size": 1024,
      "distance": "Cosine"
    },
    "shard_number": 2,
    "optimizers_config": {
        "indexing_threshold": 0
    }
}

// List points in a collection, using filter by metadata
POST collections/justicio/points/scroll
{
  "limit": 40000,
  "filter": {
    "must": [
      {
        "key": "metadata.anio",
        "match": {
          "value": "2018"
        }
      }
    ]
  }
}

// Count points in a collection, using filter by metadata
POST collections/justicio/points/count
{
  "filter": {
    "must": [
      {
        "key": "metadata.anio",
        "match": {
          "value": "2018"
        }
      }
    ]
  }
}

// Count points in a collection, using filter by multiple metadata
POST collections/justicio/points/count
{
  "filter": {
    "must": [
      {
        "key": "metadata.anio",
        "match": {
          "value": "2024"
        }
      },
      {
        "key": "metadata.mes",
        "match": {
          "value": "02"
        }
      },
      {
        "key": "metadata.dia",
        "match": {
          "value": "20"
        }
      }
    ]
  }
}

// Delete points in a collection, using filter by metadata
POST collections/justicio/points/delete
{
  "filter": {
    "must": [
      {
        "key": "metadata.anio",
        "match": {
          "value": "2018"
        }
      }
    ]
  }
}

// No funciona porque tenemos las fechas como strings en lugar de como n√∫meros.
POST /collections/boe-bge-m3/points/count
{
    "filter": {
        "should": [
            {
                "key": "metadata.anio",
                "range": {
                    "gte": 2000
                }
            }
        ]
    }
}


